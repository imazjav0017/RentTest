<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rent.rentmanagement.renttest">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:configChanges="orientation|screenSize"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:resizeableActivity="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".HomePageActivity" android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"   android:resizeableActivity="false"/>
        <activity android:name=".BuildActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".roomActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>

        <supports-screens
            android:anyDensity="true"
            android:largeScreens="true"
            android:normalScreens="true"
            android:smallScreens="true"
            android:xlargeScreens="true" />

        <activity android:name=".roomDetailActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".StudentActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".TotalTenantsctivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".edit_rooms"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".studentProfile"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".MainActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".automanualActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".manualActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
        <activity android:name=".buildingActivity"  android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"  android:resizeableActivity="false"/>
    </application>

</manifest>